select t1.Company,
		t1.FirstName || ' ' || t1.LastName as Cliente,
		sum(t2.Total) as Receita_Total

from Customer as t1

left join Invoice as t2
on t1.CustomerId = t2.CustomerId

where Company is not null 
group by t1.Company 
order by Receita_Total DESC
limit 10;</sql><current_tab id="0"/></tab_sql></sqlb_project>
