select 	t1.FirstName || ' ' || t1.LastName as Cliente,
		t1.Country,
		sum(t2.Total) as Receita_Total
from Customer as t1

left join Invoice as t2
on t1.CustomerId = t2.CustomerId 

group by t1.CustomerId, t1.Country

order by Receita_Total desc</sql><current_tab id="0"/></tab_sql></sqlb_project>
