select t1.FirstName || ' ' || t2.LastName as Vendedor,
		sum(t3.Total) as ReceitaTotal
from Employee as t1

left join Customer as t2
on t1.EmployeeId = t2.SupportRepId

left join Invoice as t3
on t2.CustomerId = t3.CustomerId

left join InvoiceLine as t4
on t3.InvoiceId = t4.InvoiceId

where title = 'Sales Support Agent'
group by t1.EmployeeId
order by ReceitaTotal desc
</sql><current_tab id="0"/></tab_sql></sqlb_project>
